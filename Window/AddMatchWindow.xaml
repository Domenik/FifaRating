<Window x:Class="FifaRating.AddMatchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:fifaRating="clr-namespace:FifaRating"
        Title="AddMatchWindow" Height="200" Width="400"
        Background="#333">
    <DockPanel>
        <fifaRating:AddMatchView DockPanel.Dock="Top"/> 
        <Button Content="Добавить"
                Width="120" Height="30"
                Click="AddMatchResults"/>
    </DockPanel>
</Window>
