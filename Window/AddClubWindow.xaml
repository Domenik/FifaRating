<Window x:Class="FifaRating.AddClubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:fifaRating="clr-namespace:FifaRating"
        Title="AddClubWindow" Width="500" Height="200" 
        Background="#333">
    <DockPanel>
        <fifaRating:AddClubView DockPanel.Dock="Top" DataContext="{Binding}"/>
        <Button Content="Добавить" Width="120" Height="30" Click="AddClubToList"/>
    </DockPanel>
</Window>
